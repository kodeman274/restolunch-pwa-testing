{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true
  },
  "extends": ["airbnb-base", "plugin:codeceptjs/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "indent": "off",
    "linebreak-style": 0,
    "object-curly-newline": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-useless-path-segments": 0,
    "import/prefer-default-export": 0,
    "eol-last": 0,
    "func-names": 0,
    "no-trailing-spaces": 0,
    "no-undef": 0
  }
}
